{
	"data": {
		"filename": "aapl_stock.csv",
		"columns": [
			"Close","Open","High","Low"
		],
		"sequence_length": 30,
		"train_test_split": 0.85,
		"normalise": true
	},
	"training": {
		"epochs": 1,
		"batch_size": 8
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 100,
				"input_timesteps": 29,
				"input_dim": 4,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	},
	"companies":{
		"name":["AAPL","ADBE","CINF","EBAY","FFIV","FOX","GOOGL","TXN","NVDA","QCOM"]
	}
}
